{
  "$schema": "https://json.schemastore.org/package",
  "name": "@genkit/eslint-config-react",
  "description": "ESLint config for BAT react projects",
  "version": "0.0.7",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "eslint-config-react",
    "formatter"
  ],
  "scripts": {
    "dev": "vite build --mode development",
    "build": "vite build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.json,.jsonc,.json5 .",
    "prepublish": "pnpm run build"
  },
  "peerDependencies": {
    "@genkit/eslint-config": ">=0.0.7",
    "@types/react": ">=16.0.0",
    "@types/react-dom": ">=16.0.0",
    "eslint": ">=8.0.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "typescript": ">=4.7"
  },
  "dependencies": {
    "@genkit/eslint-config": "workspace:^",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3"
  },
  "devDependencies": {
    "@genkit/tsconfig-browser": "workspace:^",
    "@genkit/tsconfig-node": "workspace:^",
    "@types/eslint": "^8.44.1",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
