{
  "$schema": "https://json.schemastore.org/package",
  "name": "@genkit/tsconfig-node",
  "description": "TypeScript configuration for nodejs applications",
  "version": "0.0.2",
  "main": "index.json",
  "module": "index.json",
  "exports": {
    "./package.json": "./package.json",
    "./declaration": "./declaration.json",
    ".": {
      "import": "./index.json",
      "require": "./index.json"
    }
  },
  "files": [
    "./index.json",
    "./declaration.json"
  ],
  "author": {
    "name": "Jove Jonovski",
    "email": "jovejonovski@gmail.com",
    "url": "https://github.com/datyin"
  },
  "repository": {
    "directory": "configs/tsconfig-node",
    "type": "git",
    "url": "https://github.com/datyin/GenKit"
  },
  "devDependencies": {
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "typescript": ">=4.7"
  },
  "keywords": [
    "tsconfig",
    "typescript"
  ],
  "publishConfig": {
    "access": "public"
  }
}
